<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="titleColor" Color="White"/>
    <SolidColorBrush x:Key="borderColor" Color="Black"/>
    <SolidColorBrush x:Key="highlightColor" Color="Red"/>
    <SolidColorBrush x:Key="windowButton" Color="#FF276B87"/>
    <SolidColorBrush x:Key="windowButtonMouseOver" Color="#FFF"/>
    <SolidColorBrush x:Key="windowButtonMousePressed" Color="#FF1F6D8E"/>
    <SolidColorBrush x:Key="titleBackground" Color="#FF41B1E1"/>
    <SolidColorBrush x:Key="selectedButton" Color="#FFF2C9FF"/>
    <SolidColorBrush x:Key="mouseOverButtonColor" Color="#FFE5F3FB"/>
</ResourceDictionary>